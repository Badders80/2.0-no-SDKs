# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Futureverse Configuration
VITE_FV_CLIENT_ID=your_futureverse_client_id_here
VITE_FV_REDIRECT_URI=http://localhost:5173

# API Configuration
VITE_API_BASE_URL=https://api.example.com
VITE_TOKINVEST_API_URL=https://api.tokinvest.capital

# Development
VITE_DEBUG=true
VITE_LOG_LEVEL=info

# Optional: WalletConnect Project ID (if using WalletConnect)
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id
